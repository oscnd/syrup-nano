   "source": [
    "import ctypes\n",
    "\n",
    "# Load the shared library\n",
    "lib = ctypes.CDLL('../nano.so')\n",
    "\n",
    "# Define argument and return types\n",
    "lib.encode.argtype = ctypes.c_char_p\n",
    "lib.encode.restype = ctypes.c_char_p\n",
    "\n",
    "# Call the Go function\n",
    "result = lib.encode(b\"Hello\").decode('utf-8')\n",
    "print(result)"
   ]
